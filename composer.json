{
	"name": "console-helpers/svn-buddy",
	"description": "Easier way to control work with Subversion repositories from command-line",
	"license": "BSD 3-Clause",
	"authors": [
		{
			"name": "Alexander Obuhovich",
			"email": "aik.bold@gmail.com"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"symfony/console": ">=2.7",
		"symfony/process": ">=2.7",
		"pimple/pimple": ">=2.0",
		"stecman/symfony-console-completion": "~0.6@dev",
		"console-helpers/console-kit": "^0.0.1"
	},
	"require-dev": {
		"aik099/coding-standard": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"ConsoleHelpers\\SVNBuddy\\": "src/SVNBuddy/",
			"ConsoleHelpers\\ConsoleKit\\": "src/ConsoleKit/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\ConsoleHelpers\\SVNBuddy\\": "tests/SVNBuddy/",
			"Tests\\ConsoleHelpers\\ConsoleKit\\": "tests/ConsoleKit/"
		}
	},
	"bin": ["bin/svn-buddy"]
}
