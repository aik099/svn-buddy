{
	"name": "aik099/svn-buddy",
	"description": "Easier way to control work with Subversion repositories from command-line",
	"license": "BSD 3-Clause",
	"authors": [
		{
			"name": "Alexander Obuhovich",
			"email": "aik.bold@gmail.com"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"symfony/console": ">=2.7",
		"symfony/process": ">=2.7",
		"pimple/pimple": ">=2.0",
		"stecman/symfony-console-completion": "~0.6@dev"
	},
	"require-dev": {
		"mockery/mockery": "~0.9",
		"aik099/coding-standard": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"aik099\\SVNBuddy\\": "src/SVNBuddy/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\aik099\\SVNBuddy\\": "tests/SVNBuddy/"
		}
	},
	"bin": ["bin/svn-buddy"]
}
